{
    "name": "insthync/yii2-gallery-widget",
    "description": "Blueimp gallery widget for the Yii framework (Forked from https://github.com/2amigos/yii2-gallery-widget to update for latest Yii2)",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "widget",
        "gallery"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/insthync/yii2-gallery-widget",
    "authors": [
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        },
        {
            "name": "Insthync",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.45",
        "bower-asset/blueimp-gallery": "~3.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.0",
        "scrutinizer/ocular": "~1.9"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\gallery\\": "src"
        }
    },
    "extra": {
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "public/assets/{$vendor}/{$name}/": ["type:bower-asset", "type:npm-asset"]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
